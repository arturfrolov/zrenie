{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error",{
      "endOfLine": "auto",
      "singleQuote": true,
      "tabWidth": 2,
      "no-console": "off"
//      "indent": 2
      }
    ]
  }
}